{
  "name": "chatty-client",
  "version": "1.0.0",
  "description": "Cyclejs Chat Client",
  "main": "./dist/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --port 7575 --host 0.0.0.0",
    "build": "rm -rf dist/ && cross-env NODE_ENV=production webpack --progress --profile --hide-modules && cp -r ./assets dist/",
    "start": "npm run dev"
  },
  "keywords": [
    "chat",
    "streams",
    "reactive",
    "programming",
    "cyclejs"
  ],
  "author": "Calvin Karundu <calvin.mwenda@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@cycle/dom": "^18.0.0",
    "@cycle/http": "^14.0.0",
    "@cycle/run": "^3.1.0",
    "@cycle/time": "^0.8.0",
    "csstips": "^0.2.2",
    "csx": "^8.4.0",
    "lodash": "^4.17.4",
    "typestyle": "^1.3.8",
    "xstream": "^10.9.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.2",
    "html-webpack-plugin": "^2.30.1",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "typings": "^2.1.1",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1"
  }
}
